{
  "compilerOptions": {
    "target": "ES6",
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true
  }
}
